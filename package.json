{
  "name": "spiritueux",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/thomasblanchard-dev/spiritueux",
  "bugs": {
    "url": "https://github.com/thomasblanchard-dev/spiritueux/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomasblanchard-dev/spiritueux.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "yarn lint:fix && nx run-many --target=prebuild --all --parallel",
    "build": "nx run-many --target=build --all --parallel",
    "build-storybook": "nx run-many --target=build-storybook --all --parallel",
    "dep-graph": "nx dep-graph",
    "postinstall": "is-ci || husky install",
    "lint": "yarn prettier && yarn stylelint && nx run-many --target=lint --parallel",
    "lint:fix": "yarn prettier:fix && yarn stylelint:fix && nx run-many --target=lint:fix --parallel",
    "prettier": "prettier --plugin-search-dir=. .",
    "prettier:fix": "prettier --write --plugin-search-dir=. .",
    "serve": "nx run-many --target=serve --all --parallel",
    "start": "nx run-many --target=start --all --parallel",
    "stylelint": "stylelint {apps,libs}/**/*.css",
    "stylelint:fix": "yarn stylelint --fix",
    "test": "nx run-many --target=test --all --parallel"
  },
  "lint-staged": {
    "./{apps,libs}/**/*.css": [
      "yarn stylelint:fix"
    ],
    "./{apps,libs}/**/*.{json,js,jsx,ts,tsx,svelte}": [
      "yarn prettier:fix"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@nrwl/cli": "14.8.3",
    "@nrwl/cypress": "14.8.3",
    "@nrwl/eslint-plugin-nx": "14.8.3",
    "@nrwl/jest": "14.8.3",
    "@nrwl/js": "14.8.3",
    "@nrwl/linter": "14.8.3",
    "@nrwl/next": "^14.8.3",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "14.8.3",
    "@nrwl/web": "14.8.3",
    "@nrwl/workspace": "14.8.3",
    "@testing-library/react": "13.4.0",
    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "babel-jest": "28.1.1",
    "cypress": "^10.7.0",
    "eslint": "~8.15.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "lint-staged": "^13.0.3",
    "nx": "14.8.3",
    "prettier": "^2.6.2",
    "react-test-renderer": "18.2.0",
    "sass": "1.55.0",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  },
  "engines": {
    "node": "^14 || ^15 || ^16",
    "npm": "please-use-yarn",
    "yarn": ">= 1.19.1"
  },
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "dependencies": {
    "autoprefixer": "^10.4.12",
    "core-js": "^3.6.5",
    "next": "12.3.1",
    "postcss": "^8.4.17",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "regenerator-runtime": "0.13.7",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.3.0"
  }
}
