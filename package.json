{
  "name": "spiritueux",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/thomasblanchard-dev/spiritueux",
  "bugs": {
    "url": "https://github.com/thomasblanchard-dev/spiritueux/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomasblanchard-dev/spiritueux.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "yarn lint:fix && nx run-many --target=prebuild --all --parallel",
    "build": "nx run-many --target=build --all --parallel",
    "build-storybook": "nx run-many --target=build-storybook --all --parallel",
    "dep-graph": "nx dep-graph",
    "postinstall": "is-ci || husky install",
    "lint": "yarn prettier && yarn stylelint && nx run-many --target=lint --parallel",
    "lint:fix": "yarn prettier:fix && yarn stylelint:fix && nx run-many --target=lint:fix --parallel",
    "prettier": "prettier --plugin-search-dir=. .",
    "prettier:fix": "prettier --write --plugin-search-dir=. .",
    "serve": "nx run-many --target=serve --all --parallel",
    "start": "nx run-many --target=start --all --parallel",
    "storybook": "nx run-many --target=storybook --all --parallel",
    "storybook:ui": "nx storybook ui",
    "stylelint": "stylelint {apps,libs}/**/*.css",
    "stylelint:fix": "yarn stylelint --fix",
    "test": "nx run-many --target=test --all --parallel"
  },
  "lint-staged": {
    "./{apps,libs}/**/*.css": [
      "yarn stylelint:fix"
    ],
    "./{apps,libs}/**/*.{json,js,jsx,ts,tsx,svelte}": [
      "yarn prettier:fix"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@babel/preset-typescript": "7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@nrwl/cli": "15.0.5",
    "@nrwl/cypress": "15.0.5",
    "@nrwl/eslint-plugin-nx": "15.0.5",
    "@nrwl/jest": "15.0.5",
    "@nrwl/js": "15.0.5",
    "@nrwl/linter": "15.0.5",
    "@nrwl/next": "^15.0.5",
    "@nrwl/nx-cloud": "^15.0.1",
    "@nrwl/react": "15.0.5",
    "@nrwl/storybook": "^15.0.5",
    "@nrwl/web": "15.0.5",
    "@nrwl/workspace": "15.0.5",
    "@storybook/addon-essentials": "~6.5.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "~6.5.9",
    "@storybook/core-server": "~6.5.9",
    "@storybook/manager-webpack5": "~6.5.9",
    "@storybook/react": "~6.5.9",
    "@storybook/test-runner": "^0.9.0",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.2.2",
    "@types/node": "18.11.9",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "babel-jest": "29.3.0",
    "babel-loader": "9.0.1",
    "cypress": "^10.7.0",
    "eslint": "~8.26.0",
    "eslint-config-next": "13.0.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "29.3.0",
    "jest-environment-jsdom": "29.2.2",
    "lint-staged": "^13.0.3",
    "nx": "15.0.5",
    "prettier": "^2.6.2",
    "react-test-renderer": "18.2.0",
    "sass": "1.55.0",
    "storybook-addon-sass-postcss": "^0.1.3",
    "storybook-css-modules-preset": "^1.1.1",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^29.0.0",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2",
    "url-loader": "^4.1.1"
  },
  "engines": {
    "node": "^14 || ^15 || ^16",
    "npm": "please-use-yarn",
    "yarn": ">= 1.19.1"
  },
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "dependencies": {
    "autoprefixer": "^10.4.12",
    "core-js": "^3.6.5",
    "next": "13.0.1",
    "postcss": "^8.4.17",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "regenerator-runtime": "0.13.10",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.3.0"
  }
}
